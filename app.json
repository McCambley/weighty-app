{
  "expo": {
    "name": "weighty-app",
    "slug": "weighty-app",
    "platforms": [
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/apple-touch-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a203b"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/apple-touch-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-chrome-512x512.png",
        "backgroundColor": "#7187a3"
      },
      "icon": "./assets/images/apple-touch-icon.png"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon-32x32.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/weighty-app/"
    }
  }
}